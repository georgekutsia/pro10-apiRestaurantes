{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/restaurantes(.*)", "dest": "index.js" },
    { "src": "/usuarios(.*)", "dest": "index.js" },
    { "src": "/comentarios(.*)", "dest": "index.js" },
    { "src": "/comidas(.*)", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
